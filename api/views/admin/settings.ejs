<%- include('./partials/header') %>
<%- include('./partials/menu') %>
		<div class="wrapper wrapper-content">
			 <div class="row">
			 	  <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
			 	  	   <div class="ibox">
			 	  	   	    <div class="ibox-content" id="history-a">
			 	  	   	    	<div class="box-heading">
		 	  	   	    	      	<h2 class="m-b-sm "> Settings</h2>
		 	  	   	    	    </div>              
                                <div class="form-group ">
                                    <label>Reward Tokens:</label>
                                    <label><%= rewartToken ? rewartToken : '' %></label>
                                </div>
                                <form id="deposit-reward-form" role="form">
                                    <div class="form-group  m-t-md ">
                                        <label>Deposit Reward Tokens:</label>
                                        <input type="text" name="depositRewardAmount" class="form-control" placeholder="">
                                    </div>
                                    <div class="clearfix d-flex-center  m-t-md m-b-lg">
                                        <button type="button"  class="btn btn-primary m-r-sm" onclick="return depositReward(this)">Deposit</button>
                                        <div id="deposit-loader"></div>  
                                    </div>
                                </form>
                                <h4></h4>
                                <form id="withdraw-reward-form" role="form">
                                    <div class="form-group  m-t-md ">
                                        <label>Withdraw Rewards Tokens:</label>
                                        <input type="text" name="withdrawRewardAmount" class="form-control" placeholder="">
                                    </div>
                                    <div class="clearfix d-flex-center m-t-md m-b-lg">
                                        <button type="button" class="btn  btn-primary  m-r-sm" onclick="return withdrawReward(this)">Withdraw</button>
                                        <div id="withdraw-loader"></div> 
                                    </div>
                                </form>
                            </div>
			 	  	   </div>
			 	  </div>
			 </div>
		</div>
    </div>
 </div>
<div class="modal coman-modal  fade" id="Status-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" >
    <div class="modal-dialog modal-sm modal-a" role="document">
        <div class="modal-content text-center">
             <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
            <h2>Status</h2>
            <div class="form-group text-left">
                <label> <span>Document 1 :</span> Approve</label>
            </div>
            <div class="form-group text-left">
                <label><span>Document 2 : </span> Reject Image is Blur</label>
                </div>
                    <div class="form-group  text-left">
                <label><span>Document 3 : </span> Approve</label> 
            </div>
        </div>
    </div>
</div>
<div class="modal coman-modal fade" id="modal-delete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content text-center">
            <h3>Are you sure you want to Delete Asset    Owner?</h3>
            <div class="clearfix">
                <button type="submit" class="btn   btn-default m-r-sm" data-dismiss="modal" >No</button>
                <button type="submit" class="btn btn-primary">Yes</button>
            </div>
        </div>
    </div>
</div>
<div class="modal coman-modal  fade" id="cong-modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" >
    <div class="modal-dialog  modal-a" role="document">
        <div class="modal-content text-center">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
            <div class="success-msg-icon">
                <i class="fa fa-check-circle m-b-0" aria-hidden="true"></i>
             </div>
             <h2>Token Sent successfully !!</h2>
        </div>
    </div>
</div>
<div class="modal fade" id="successModal" role="dialog">
    <div class="modal-dialog  text-center">
        <div class="modal-content pad-20  text-center">
            <div class=" text-center">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            </div>
            <div class="success-msg-icon">
            <i class="fa fa-check-circle m-b-0" aria-hidden="true"></i>
            </div>
            <h2><label class="title"></label></h2>
        </div>
    </div>
</div>
<%- include('./partials/footer') %>
